@model List<Entidades.Vehiculo>

@{
    ViewBag.Title = "ListaVehiculos";
    Layout = "~/Views/layout/layoutMenu.cshtml";
}

<h2>ListaVehiculos</h2>

<table border="1">
    <tr>
        <th>ID Vehículo</th>
        <th>Placa</th>
        <th>Dueño</th>
        <th>Marca</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID_Vehiculo</td>
            <td>@item.Placa</td>
            <td>@item.Dueno</td>
            <td>@item.Marca</td>
        </tr>
    }
</table>

